{
  "name": "@glyphjs/components",
  "version": "0.0.0",
  "description": "Built-in UI components for Glyph JS (graph, table, chart, etc.)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src/",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@glyphjs/runtime": "workspace:*",
    "@glyphjs/schemas": "workspace:*",
    "@glyphjs/types": "workspace:*",
    "d3": "^7.9.0",
    "dagre": "^0.8.5"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "sideEffects": false,
  "license": "MIT",
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/dagre": "^0.7.53",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3"
  }
}
