{
  "name": "@glyphjs/components",
  "version": "0.7.1",
  "description": "Built-in UI components for Glyph JS (graph, table, chart, etc.)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src/",
    "clean": "rm -rf dist .turbo",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@glyphjs/runtime": "workspace:*",
    "@glyphjs/schemas": "workspace:*",
    "@glyphjs/types": "workspace:*",
    "d3": "^7.9.0",
    "dagre": "^0.8.5",
    "elkjs": "^0.11.0",
    "katex": "^0.16.28"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "sideEffects": false,
  "license": "MIT",
  "author": "Vledic Franco",
  "repository": {
    "type": "git",
    "url": "https://github.com/VledicFranco/glyphjs.git",
    "directory": "packages/components"
  },
  "homepage": "https://github.com/VledicFranco/glyphjs#readme",
  "bugs": "https://github.com/VledicFranco/glyphjs/issues",
  "keywords": [
    "glyph",
    "glyphjs",
    "markdown",
    "react",
    "ui-components",
    "interactive-documents",
    "d3",
    "graph",
    "chart",
    "table",
    "components"
  ],
  "devDependencies": {
    "@storybook/addon-a11y": "^8",
    "@storybook/addon-essentials": "^8",
    "@storybook/react": "^8",
    "@storybook/react-vite": "^8",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7.4.3",
    "@types/dagre": "^0.7.53",
    "@types/katex": "^0.16.8",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "jsdom": "^27.4.0",
    "storybook": "^10"
  }
}
