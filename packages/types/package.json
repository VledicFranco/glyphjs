{
  "name": "@glyphjs/types",
  "version": "0.0.0",
  "description": "Shared TypeScript type definitions for Glyph JS",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc --emitDeclarationOnly --outDir dist",
    "typecheck": "tsc --noEmit",
