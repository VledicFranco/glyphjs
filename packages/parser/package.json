{
  "name": "@glyphjs/parser",
  "version": "0.0.0",
  "description": "Markdown parser with remark plugin for ui: fenced blocks",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@glyphjs/types": "workspace:*",
    "unified": "^11.0.5",
    "remark-parse": "^11.0.0",
    "remark-frontmatter": "^5.0.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/mdast": "^4.0.4",
    "@types/unist": "^3.0.3"
  },
  "sideEffects": false,
  "license": "MIT"
}
