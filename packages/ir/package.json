{
  "name": "@glyphjs/ir",
  "version": "0.0.0",
  "description": "IR utilities: validation, diffing, patching, migration",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src/",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@glyphjs/types": "workspace:*"
  },
  "devDependencies": {
    "@types/node": ">=25.2.0",
    "fast-check": "^4.5.3"
  },
  "sideEffects": false,
  "license": "MIT"
}
